{
  "name": "@usharma124/ui-qa",
  "version": "2.0.0",
  "description": "AI-powered UI/UX testing CLI with beautiful TUI",
  "type": "module",
  "bin": {
    "ui-qa": "./dist/cli-ink.js"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "bun run src/cli-ink.tsx",
    "qa": "bun run src/cli-ink.tsx",
    "dev": "bun run --watch src/cli-ink.tsx",
    "build": "bun build src/cli-ink.tsx --outdir dist --target node --packages external && chmod +x dist/cli-ink.js",
    "prepublishOnly": "bun run build",
    "test": "bun test",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "dependencies": {
    "@axe-core/playwright": "^4.10.2",
    "axe-core": "^4.10.2",
    "chalk": "^5.6.2",
    "dotenv": "^17.2.3",
    "figures": "^6.1.0",
    "ink": "^6.6.0",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "^6.0.0",
    "openai": "^6.16.0",
    "playwright": "^1.58.0",
    "react": "^19.2.4",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/node": "^25.1.0",
    "@types/react": "^19.2.10",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitepress": "^1.6.4"
  }
}
